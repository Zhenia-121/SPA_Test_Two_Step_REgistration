{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=TwoStepRegistrationWithIdentity;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "PasswordRequirements": {
    "RequireDigit": true,
    "RequiredLength": 2,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false,
    "RequireLowercase": false,
    "Regex": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{2,}$"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    }
  }
}
